{
  "name": "apitte/presenter",
  "description": "ApiPresenter for Nette framework",
  "keywords": [
    "nette",
    "api"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/apitte/presenter",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.3",
    "apitte/core": "~0.7 || ~0.8",
    "nette/application": "^3.0.0",
    "nette/http": "^3.0.1"
  },
  "require-dev": {
    "ninjify/nunjuck": "^0.4",
    "ninjify/qa": "^0.12",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-nette": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "Apitte\\Presenter\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
